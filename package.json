{
  "name": "@aspen.cloud/aspen-cli",
  "description": "Backup and fetch all of your data from Gmail, Spotify, and more into a single database",
  "version": "0.1.2",
  "author": "Aspen @aspen-cloud",
  "bin": {
    "aspen": "./bin/run"
  },
  "bugs": "https://github.com/aspen-cloud/aspen-cli/issues",
  "dependencies": {
    "@aspen.cloud/aspendb": "^0.1.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-plugins": "^1.7.9",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/listr": "^0.14.2",
    "cli-ux": "^5.4.5",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.4",
    "listr": "^0.14.3",
    "platform-folders": "^0.4.1",
    "rxjs": "^6.5.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.10",
    "@types/mocha": "^7.0.1",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aspen-cloud/aspen-cli",
  "keywords": [
    "aspen",
    "cloud",
    "api",
    "gmail",
    "spotify",
    "cli"
  ],
  "license": "Apache License 2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "aspen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "aspen-cloud/aspen-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only test/**/*.test.ts",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
